{
    "name": "drew/debug-statements-fixers",
    "description": "Removes debug statements, which shouldn't be in production ever",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Kovalyov",
            "email": "andrew.kovalyoff@gmail.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Drew\\DebugStatementsFixers\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drew\\RemoveDebugStatements\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls": "^2.4"
    }
}
